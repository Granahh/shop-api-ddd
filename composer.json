{
  "name": "granahh/shop-api-ddd",
  "license": "MIT",
  "type": "project",
  "description": "Cart Shop API DDD",
  "require": {
    "php": "^8.0",
    "ext-amqp": "*",
    "ext-apcu": "*",
    "ext-json": "*",
    "ext-zend-opcache": "*",
    "ext-pdo": "*",
    "symfony/framework-bundle": "^5.3",
    "symfony/messenger": "^5.3",
    "symfony/dotenv": "^5.3",
    "symfony/yaml": "^5.3",
    "symfony/validator": "^5.3",
    "lambdish/phunctional": "^2.0",
    "ramsey/uuid": "^3.9",
    "doctrine/dbal": "^2.10",
    "doctrine/orm": "^2.7",
    "mockery/mockery": "^1.5",
    "fzaninotto/faker": "^1.5",
    "symfony/test-pack": "^1.0"
  },
  "require-dev": {
    "ext-xdebug": "*",
    "roave/security-advisories": "dev-master",
    "behat/behat": "^3.6",
    "friends-of-behat/mink-extension": "^2.4",
    "behat/mink-browserkit-driver": "^1.3",
    "friends-of-behat/symfony-extension": "^2.1",
    "phpunit/phpunit": "^9.2",
    "symfony/debug": "^4.4"
  },
  "autoload": {
    "psr-4": {
      "Granah\\Apps\\CartShop\\Backend\\": "apps/cart_shop/backend/src",
      "Granah\\": [
        "src"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Granah\\CartShop\\Tests\\": [
        "tests/src"
      ]
    }
  }
}
